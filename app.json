{
  "expo": {
    "name": "expo28",
    "description": "This project is really great.",
    "slug": "expo28",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.expo.turtle.cli.example"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expo.turtle.cli.example",
      "config": {
        "usesNonExemptEncryption": false
      }
    }
  }
}
